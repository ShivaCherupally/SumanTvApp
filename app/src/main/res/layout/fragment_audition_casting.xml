                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginLeft="@dimen/_5sdp"
                android:padding="10dp">

            <info.dkapp.flow.fontstyles.TextViewBold
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="9dp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:visibility="gone"
                    android:background="@null"
                    android:text="Talent"
                    android:textColor="@color/skyblueNew"
                    android:textSize="@dimen/_16sdp"/>

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone">

                <RelativeLayout
                        android:id="@+id/rCreateProfile"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal"
                        android:padding="5dp">

                    <RelativeLayout
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_centerVertical="true">

                        <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:alpha="0.3"
                                android:background="@drawable/circle_white"
                                android:backgroundTint="@color/color_eee"
                                android:visibility="gone"
                                android:tint="@color/black_light"/>

                        <ImageView
                                android:layout_width="36dp"
                                android:layout_height="36dp"
                                android:layout_marginLeft="3dp"
                                android:layout_marginTop="3dp"
                                android:padding="@dimen/_5sdp"
                                android:src="@drawable/a_create_profile"
                                android:tint="@color/auditions_color"/>

                    </RelativeLayout>

                    <info.dkapp.flow.fontstyles.TextViewSemiBold
                            android:id="@+id/createtxt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_30sdp"
                            android:background="@null"
                            android:backgroundTint="@color/skyblueNew"
                            android:drawablePadding="@dimen/_14sdp"
                            android:textSize="@dimen/_14sdp"
                            android:gravity="center_vertical"
                            android:padding="10dp"
                            android:text="Create Profile"
                            android:textColor="@color/auditions_color"
                            />

                </RelativeLayout>

                <LinearLayout
                        android:id="@+id/llUploadingMedia"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white">

                    <info.dkapp.flow.fontstyles.TextViewMedium
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="15sp"
                            android:layout_weight="1"
                            android:text="@string/uploading_media_please_wait"
                            android:textColor="@color/light_new_gray"
                            android:textSize="14sp"/>

                    <ProgressBar
                            android:id="@+id/progressBar"
                            android:layout_width="35dp"
                            android:layout_height="35dp"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="10dp"
                            android:indeterminate="true"
                            android:indeterminateTint="@color/app_snack_bar_true"
                            android:padding="5dp"/>

                </LinearLayout>

                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:background="@color/light_auditions"/>

                <RelativeLayout
                        android:id="@+id/rNotification"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:orientation="horizontal"
                        android:padding="5dp">

                    <RelativeLayout
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_centerVertical="true">

                        <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:alpha="0.3"
                                android:visibility="gone"
                                android:background="@drawable/circle_white"
                                android:backgroundTint="@color/color_eee"
                                android:tint="#666666"/>

                        <TextView
                                android:id="@+id/notificationCount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_marginBottom="@dimen/_5sdp"
                                android:layout_alignParentTop="true"
                                android:text=""
                                android:visibility="gone"
                                android:textSize="@dimen/_10sdp"
                                android:textColor="@color/skyblueNew"
                                android:textStyle="bold"/>


                        <ImageView
                                android:layout_width="32dp"
                                android:layout_height="32dp"
                                android:layout_centerVertical="true"
                                android:padding="@dimen/_5sdp"
                                android:id="@+id/ivNotificationIcon"
                                app:srcCompat="@drawable/ic_notification"
                                />

                    </RelativeLayout>

                    <info.dkapp.flow.fontstyles.TextViewSemiBold
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_30sdp"
                            android:background="@null"
                            android:backgroundTint="@color/skyblueNew"
                            android:drawablePadding="@dimen/_14sdp"
                            android:gravity="center_vertical"
                            android:padding="10dp"
                            android:text="Recommendations"
                            android:textColor="@color/auditions_color"
                            android:textSize="@dimen/_14sdp"/>

                </RelativeLayout>

                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:background="@color/light_auditions"/>


                <RelativeLayout
                        android:id="@+id/rMyAuditions"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="3dp"
                        android:orientation="horizontal"
                        android:padding="5dp">

                    <RelativeLayout
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_centerVertical="true">

                        <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:alpha="0.3"
                                android:visibility="gone"
                                android:background="@drawable/circle_white"
                                android:backgroundTint="@color/color_eee"
                                android:tint="#666666"/>

                        <ImageView
                                android:layout_width="36dp"
                                android:layout_height="36dp"
                                android:layout_centerVertical="true"
                                android:padding="@dimen/_5sdp"
                                android:src="@drawable/a_my_auditions"
                                android:tint="@color/auditions_color"/>

                    </RelativeLayout>

                    <info.dkapp.flow.fontstyles.TextViewSemiBold
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_30sdp"
                            android:background="@null"
                            android:backgroundTint="@color/skyblueNew"
                            android:drawablePadding="@dimen/_14sdp"
                            android:gravity="center_vertical"
                            android:padding="10dp"
                            android:text="My Auditions"
                            android:textColor="@color/auditions_color"
                            android:textSize="@dimen/_14sdp"/>

                </RelativeLayout>

                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:background="@color/light_auditions"/>

                <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                    <RelativeLayout
                            android:id="@+id/rMyIntrests"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="3dp"
                            android:orientation="horizontal"
                            android:padding="5dp">

                        <RelativeLayout
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:layout_centerVertical="true">

                            <ImageView
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:alpha="0.3"
                                    android:visibility="gone"
                                    android:background="@drawable/circle_white"
                                    android:backgroundTint="@color/color_eee"
                                    android:tint="#666666"/>

                            <ImageView
                                    android:layout_width="32dp"
                                    android:layout_height="32dp"
                                    android:layout_centerVertical="true"
                                    android:padding="@dimen/_5sdp"
                                    android:src="@drawable/a_my_intrests"
                                    android:tint="@color/auditions_color"/>

                        </RelativeLayout>

                        <info.dkapp.flow.fontstyles.TextViewSemiBold
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/_30sdp"
                                android:background="@null"
                                android:backgroundTint="@color/skyblueNew"
                                android:drawablePadding="@dimen/_14sdp"
                                android:gravity="center_vertical"
                                android:padding="10dp"
                                android:text="My Interests"
                                android:textColor="@color/auditions_color"
                                android:textSize="@dimen/_14sdp"/>

                        <ImageView
                                android:id="@+id/ivMyIntrests"
                                android:layout_width="@dimen/_14sdp"
                                android:layout_height="17dp"
                                android:layout_marginRight="@dimen/_5sdp"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                app:srcCompat="@drawable/ic_rigth_arrow"
                                android:tint="@color/auditions_color"/>


                    </RelativeLayout>

                    <LinearLayout
                            android:id="@+id/llMyIntrests"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_5sdp"
                            android:layout_marginLeft="@dimen/_20sdp"
                            android:visibility="gone">

                        <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/recyclerView"
                                android:layout_marginLeft="@dimen/_18sdp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                        </androidx.recyclerview.widget.RecyclerView>

                    </LinearLayout>

                </LinearLayout>

                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:background="@color/light_auditions"/>

                <RelativeLayout
                        android:id="@+id/rAuditionSearch"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="3dp"
                        android:orientation="horizontal"
                        android:padding="5dp">

                    <RelativeLayout
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_centerVertical="true">

                        <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:alpha="0.3"
                                android:visibility="gone"
                                android:background="@drawable/circle_white"
                                android:backgroundTint="@color/color_eee"
                                android:tint="#666666"/>

                        <ImageView
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_centerVertical="true"
                                android:padding="@dimen/_5sdp"
                                app:srcCompat="@drawable/ic_audition_search"
                                android:tint="@color/auditions_color"/>

                    </RelativeLayout>

                    <info.dkapp.flow.fontstyles.TextViewSemiBold
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_30sdp"
                            android:background="@null"
                            android:backgroundTint="@color/skyblueNew"
                            android:drawablePadding="@dimen/_14sdp"
                            android:gravity="center_vertical"
                            android:padding="10dp"
                            android:text="Audition Search"
                            android:textColor="@color/auditions_color"
                            android:textSize="@dimen/_14sdp"/>

                </RelativeLayout>

                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:background="@color/light_auditions"/>


                <RelativeLayout
                        android:id="@+id/rFavouriteAudition"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="3dp"
                        android:orientation="horizontal"
                        android:padding="5dp">

                    <RelativeLayout
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_centerVertical="true">

                        <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:alpha="0.3"
                                android:visibility="gone"
                                android:background="@drawable/circle_white"
                                android:backgroundTint="@color/color_eee"
                                android:tint="#666666"/>

                        <ImageView
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_centerVertical="true"
                                android:padding="@dimen/_5sdp"
                                app:srcCompat="@drawable/ic_fav_blue"
                                android:tint="@color/auditions_color"/>

                    </RelativeLayout>

                    <info.dkapp.flow.fontstyles.TextViewSemiBold
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_30sdp"
                            android:background="@null"
                            android:backgroundTint="@color/skyblueNew"
                            android:drawablePadding="@dimen/_14sdp"
                            android:gravity="center_vertical"
                            android:padding="10dp"
                            android:text="Favourites"
                            android:textColor="@color/auditions_color"
                            android:textSize="@dimen/_14sdp"/>

                </RelativeLayout>

                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:background="@color/light_auditions"/>

            </LinearLayout>

            <RelativeLayout
                    android:visibility="gone"
                    android:id="@+id/rlChat"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:orientation="horizontal"
                    android:padding="5dp">

                <RelativeLayout
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_centerVertical="true">

                    <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:alpha="0.3"
                            android:visibility="gone"
                            android:background="@drawable/circle_white"
                            android:backgroundTint="@color/color_eee"
                            android:tint="#666666"/>

                    <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="5dp"
                            android:padding="@dimen/_5sdp"
                            android:src="@drawable/a_search"
                            android:tint="@color/auditions_color"/>

                </RelativeLayout>

                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_30sdp"
                        android:background="@null"
                        android:backgroundTint="@color/skyblueNew"
                        android:drawablePadding="@dimen/_14sdp"
                        android:fontFamily="@font/sf_semibold"
                        android:gravity="center_vertical"
                        android:padding="10dp"
                        android:text="CHAT (TESTING)"
                        android:textColor="@color/auditions_color"
                        android:textSize="@dimen/_14sdp"/>

            </RelativeLayout>

            <View
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:background="@color/light_auditions"/>

            <info.dkapp.flow.fontstyles.TextViewBold
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@null"
                    android:backgroundTint="@color/skyblueNew"
                    android:drawablePadding="@dimen/_14sdp"
                    android:text="Casting"
                    android:visibility="gone"
                    android:textColor="@color/skyblueNew"
                    android:textSize="@dimen/_16sdp"/>

            <RelativeLayout
                    android:id="@+id/rPostAnAudition"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:orientation="horizontal"
                    android:padding="5dp">

                <RelativeLayout
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_centerVertical="true">

                    <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:alpha="0.3"
                            android:visibility="gone"
                            android:background="@drawable/circle_white"
                            android:backgroundTint="@color/color_eee"
                            android:tint="#666666"/>

                    <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_centerVertical="true"
                            android:padding="@dimen/_4sdp"
                            android:src="@drawable/a_post_audition"
                            android:tint="@color/auditions_color"/>

                </RelativeLayout>

                <info.dkapp.flow.fontstyles.TextViewSemiBold
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_30sdp"
                        android:background="@null"
                        android:backgroundTint="@color/skyblueNew"
                        android:drawablePadding="@dimen/_14sdp"
                        android:gravity="center_vertical"
                        android:padding="10dp"
                        android:text="Create Audition"
                        android:textColor="@color/auditions_color"
                        android:textSize="@dimen/_14sdp"/>

            </RelativeLayout>

            <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:background="@color/light_auditions"/>

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:orientation="vertical"
                    android:padding="5dp">

                <RelativeLayout
                        android:id="@+id/rMyAudition"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                    <RelativeLayout
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_centerVertical="true">

                        <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:alpha="0.3"
                                android:visibility="gone"
                                android:background="@drawable/circle_white"
                                android:backgroundTint="@color/color_eee"
                                android:tint="#666666"/>

                        <ImageView
                                android:layout_width="36dp"
                                android:layout_height="36dp"
                                android:layout_centerVertical="true"
                                android:padding="@dimen/_3sdp"
                                android:src="@drawable/a_my_auditions"
                                android:tint="@color/auditions_color"/>

                    </RelativeLayout>

                    <info.dkapp.flow.fontstyles.TextViewSemiBold
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_30sdp"
                            android:background="@null"
                            android:backgroundTint="@color/skyblueNew"
                            android:drawablePadding="@dimen/_14sdp"
                            android:gravity="center_vertical"
                            android:padding="10dp"
                            android:text="My Auditions"
                            android:textColor="@color/auditions_color"
                            android:textSize="@dimen/_14sdp"/>

                    <ImageView
                            android:id="@+id/ivMyAudition"
                            android:layout_width="@dimen/_14sdp"
                            android:layout_height="17dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/_5sdp"
                            app:srcCompat="@drawable/ic_rigth_arrow"
                            android:tint="@color/auditions_color"/>

                </RelativeLayout>

                <LinearLayout
                        android:id="@+id/llMyAuditions"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:animateLayoutChanges="true"
                        android:orientation="vertical"
                        android:paddingLeft="@dimen/_10sdp"
                        android:visibility="gone">

                    <Button
                            android:id="@+id/btnDrafts"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_25sdp"
                            android:layout_marginLeft="@dimen/_22sdp"
                            android:background="@null"
                            android:text="  Drafts"
                            android:textAlignment="textStart"
                            android:fontFamily="@font/sf_semibold"
                            android:textSize="@dimen/_13sdp"
                            android:textColor="@color/auditions_color"/>

                    <Button
                            android:id="@+id/btnPosts"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_25sdp"
                            android:layout_marginLeft="@dimen/_22sdp"
                            android:background="@null"
                            android:text="  Posted"
                            android:fontFamily="@font/sf_semibold"
                            android:textSize="@dimen/_13sdp"
                            android:textAlignment="textStart"
                            android:layout_marginBottom="@dimen/_10sdp"
                            android:textColor="@color/auditions_color"/>

                </LinearLayout>

            </LinearLayout>

            <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:background="@color/light_auditions"/>


            <RelativeLayout
                    android:id="@+id/rSearchTalent"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:orientation="horizontal"
                    android:padding="5dp">

                <RelativeLayout
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginLeft="2dp"
                        android:layout_centerVertical="true">

                    <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:alpha="0.3"
                            android:visibility="gone"
                            android:background="@drawable/circle_white"
                            android:backgroundTint="@color/color_eee"
                            android:tint="#666666"/>

                    <ImageView
                            android:layout_width="36dp"
                            android:layout_height="36dp"
                            android:layout_centerVertical="true"
                            android:padding="@dimen/_4sdp"
                            android:src="@drawable/a_search_talent"
                            android:tint="@color/auditions_color"/>

                </RelativeLayout>

                <info.dkapp.flow.fontstyles.TextViewSemiBold
                        android:id="@+id/tvvvv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_30sdp"
                        android:background="@null"
                        android:backgroundTint="@color/skyblueNew"
                        android:drawablePadding="@dimen/_14sdp"
                        android:gravity="center_vertical"
                        android:padding="10dp"
                        android:text="Search Talent"
                        android:textColor="@color/auditions_color"
                        android:textSize="@dimen/_14sdp"/>


            </RelativeLayout>


            <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/light_auditions"/>

            <RelativeLayout
                    android:id="@+id/rSearchTalentFeveriotes"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:orientation="horizontal"
                    android:padding="5dp">

                <RelativeLayout
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginLeft="2dp"
                        android:layout_centerVertical="true">

                    <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:alpha="0.3"
                            android:visibility="gone"
                            android:background="@drawable/circle_white"
                            android:backgroundTint="@color/color_eee"
                            android:tint="#666666"/>

                    <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_centerVertical="true"
                            android:padding="@dimen/_5sdp"
                            app:srcCompat="@drawable/ic_fav_blue"
                            android:tint="@color/auditions_color"


                            />


                </RelativeLayout>

                <TextView
                        android:id="@+id/tvvvvvv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_30sdp"
                        android:background="@null"
                        android:backgroundTint="@color/skyblueNew"
                        android:drawablePadding="@dimen/_14sdp"
                        android:fontFamily="@font/sf_semibold"
                        android:gravity="center_vertical"
                        android:padding="10dp"
                        android:text="Favourites"
                        android:textColor="@color/auditions_color"
                        android:textSize="@dimen/_14sdp"/>


            </RelativeLayout>

            <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="@dimen/_5sdp"
