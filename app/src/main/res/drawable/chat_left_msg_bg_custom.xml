<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools" >

    <item
            android:bottom="0dp"
            android:left="@dimen/_10sdp"
    android:right="0dp"
    android:top="0dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners
                    android:topLeftRadius="0dp"
                    android:topRightRadius="@dimen/_10sdp"
                    android:bottomRightRadius="@dimen/_10sdp"
                    android:bottomLeftRadius="@dimen/_10sdp"/>
        </shape>
    </item>

    <item
            android:bottom="@dimen/_50sdp"
            android:left="0dp"
            android:right="@dimen/_50sdp"
            android:top="0dp"
            android:gravity="left|top">
        <vector xmlns:android="http://schemas.android.com/apk/res/android"
                android:width="@dimen/_25sdp"
                android:height="@dimen/_25sdp"
                android:viewportWidth="1.0"
                android:viewportHeight="1.0">
            <path
                    android:pathData="M50,50 50,0 0,0z"
                    android:fillColor="@color/white"/>
        </vector>
    </item>

</layer-list>